 }, {\n            title: \"About\",\n            list: [{\n              src: \"/image/关于.png\"\n            }]\n          }];\n        } else {\n          this.mainClass = \"col-xs-4\";\n          this.bodyClass = \"col-xs-12\";\n          this.margintophr = \"margin-top:100px\";\n          this.captionfont = \"text-align:left\";\n          this.thumbnailparentClass = \"row\";\n          this.list = [{\n            title: \"Change\"\n          }, {\n            title: \"Project\"\n          }, {\n            title: \"Logs\"\n          }, {\n            title: \"About\"\n          }];\n        }\n      }\n      if (e == 1) {\n        this.mainClass = \"col-xs-4\";\n        this.bodyClass = \"col-xs-12\";\n        this.margintophr = \"margin-top:100px\";\n        this.captionfont = \"text-align:left\";\n        this.thumbnailparentClass = \"row\";\n        this.panellist = [{\n          id: \"panelone\",\n          title: \"工程云项目接口文档\",\n          content: ` 网址 : http://epm.marketho.cn/swagger\n                            这是一个使用ASP.NET Dynamic Data技术实现的网页，使用ASP.NET WebApi技术作为接口`\n        }, {\n          id: \"paneltwo\",\n          title: \"微信云项目后台网页\",\n          content: `网址 : http://182.92.110.42\n                            这是一个使用layui框架完成的后台网页\n                            `\n        }, {\n          id: \"panelthree\",\n          title: \"微信云项目接口文档\",\n          content: `http://182.92.110.42/swagger\n                            这是使用ASP.NET WebApi实现的接口，引用swachedule包实现接口文档的显示`\n        }];\n        this.list = [{\n          title: \"Change\"\n        }, {\n          title: \"Project\"\n        }, {\n          title: \"Logs\"\n        }, {\n          title: \"About\"\n        }];\n      }\n      if (e == 2) {\n        this.mainClass = \"col-xs-4\";\n        this.bodyClass = \"col-xs-12\";\n        this.margintophr = \"margin-top:100px\";\n        this.captionfont = \"text-align:left\";\n        this.thumbnailparentClass = \"row\";\n        this.panellist = [{\n          id: \"panelone\",\n          title: \"素材库\",\n          content: `\n                            <p>VUE : https://cn.vuejs.org</p>\n                            <p>Pexels : https://www.pexels.com</p>\n                            `\n        }];\n        this.list = [{\n          title: \"Change\"\n        }, {\n          title: \"Project\"\n        }, {\n          title: \"Logs\"\n        }, {\n          title: \"About\"\n        }];\n      }\n    }\n  }\n};\n$(() => {\n  var attrList = [];\n  var list = $(\".collapseOptions\");\n  for (var i = 0; i < list.length; i++) {\n    attrList[i] = list[i].attributes.attrid.value;\n  }\n  function actionClass(thisId) {\n    $(\".collapseOptions\").parent().removeClass('action');\n    $(\".\" + thisId).parent().addClass('action');\n    return attrList.indexOf(thisId);\n  }\n  $(\".index\").on('cilck', () => {});\n  window.addEventListener('wheel', event => {\n    var thiscount = actionClass($(\".navbar-nav .action\").children().attr(\"attrId\"));\n    if (event.deltaY < 0) {\n      $(\".\" + attrList[thiscount - 1]).click();\n    } else {\n      $(\".\" + attrList[thiscount + 1]).click();\n    }\n    event.preventDefault();\n  }, {\n    passive: false\n  });\n  $(\".full-screen-div-top\").on(\"click\", function (ev) {\n    var thiscount = actionClass($(\".navbar-nav .action\").children().attr(\"attrId\"));\n    $(\".\" + attrList[thiscount - 1]).click();\n  });\n  $(\".full-screen-div-footer\").on(\"click\", function (ev) {\n    var thiscount = actionClass($(\".navbar-nav .action\").children().attr(\"attrId\"));\n    $(\".\" + attrList[thiscount + 1]).click();\n  });\n  $(\".thumbnail\").parent().on(\"mousemove\", e => {\n    $(e.currentTarget).addClass(\"athumbnail\");\n  });\n  function Sleep(milliseconds) {\n    return new Promise(resolve => setTimeout(resolve, milliseconds));\n  }\n  $(\".navbar .attrNavbar a,footer a[href='#myPage']\").on(\"click\", function (ev) {\n    ev.preventDefault();\n    var hash = this.hash;\n    actionClass(hash.substring(1, hash.length));\n    $(\"html,body\").animate({\n      scrollTop: $(hash).offset().top\n    }, 300, function () {\n      window.location.hash = hash;\n    });\n  });\n});","map":{"version":3,"names":["$","thisImage","thisDate","Date","getHours","thisStyle","data","indexImage","panellist","thumbnailparentClass","captionfont","margintophr","bodyClass","mainClass","list","title","src","methods","thumbnailSelect","e","id","content","attrList","i","length","attributes","attrid","value","actionClass","thisId","parent","removeClass","addClass","indexOf","on","window","addEventListener","event","thiscount","children","attr","deltaY","click","preventDefault","passive","ev","currentTarget","Sleep","milliseconds","Promise","resolve","setTimeout","hash","substring","animate","scrollTop","offset","top","location"],"sources":["D:\\vue_test\\project\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <body data-spy=\"scroll\" data-target=\"#example\" id=\"myPage\">\n        <nav class=\"navbar navbar-expand-lg navbar-light bg-light navbar-fixed-top\" id=\"example\">\n            <div class=\"container-fluid\">\n                <div class=\"navbar-header\">\n                    <button type=\"button\" class=\"navbar-toggle collapsed\" data-toggle=\"collapse\"\n                        data-target=\".navbar-scrollspy\">\n                        <span class=\"icon-bar\" style=\"background-color: #ccc;\"></span>\n                        <span class=\"icon-bar\" style=\"background-color: #ccc;\"></span>\n                        <span class=\"icon-bar\" style=\"background-color: #ccc;\"></span>\n                    </button>\n                    <a class=\"navbar-brand\" href=\"#\">\n                        <img alt=\"logo\" src=\"/image/logo.jpg\" class=\"img-rounded\" style=\"width: 20px;height: 20px;\">\n                    </a>\n                </div>\n                <div class=\"collapse navbar-collapse navbar-scrollspy\">\n              